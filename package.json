{
  "name": "chatapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "ioredis": "^4.9.0",
    "socket.io": "^2.2.0"
  },
  "devDependencies": {
    "concurrently": "^4.0.1"
  },
  "scripts": {
    "client": "cd client && npm run start",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
